<% @blocked.each do |user| %>
  <div>
  
    <h4><a href="/users/<%=user.id%>"><%= user.first_name %> <%= user.last_name %></a> 
    </h4>
    
<%= link_to unblock_friendship_path(user.id), method: :put do %> 
        <div class="engagement"> 
        <p> <button type="button" class="btn btn-danger btn-sm">Unblock</button> </p>
         </div>
<% end %>
    
    </div>
      
<% end %>
